<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>one.do</servlet-name>
        <servlet-class>mft.rd.controller.OneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>one.do</servlet-name>
        <url-pattern>/one.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>two.do</servlet-name>
        <servlet-class>mft.rd.controller.TwoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>two.do</servlet-name>
        <url-pattern>/two.do</url-pattern>
    </servlet-mapping>

</web-app>